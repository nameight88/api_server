{
  "manifest_version": 3,
  "name": "WalkUp Extension",
  "version": "1.0",
  "permissions":["contextMenus","storage"],
  "background": {
    "service_worker": "background.js"
  },
	"host_permissions":[
		"https://coubox.coupang.com:3010/*",
		"http://coubox.coupang.com:3010/*"
	],
  "action": {
	"default_popup":"popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
